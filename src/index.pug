doctype html
html(lang="cs")
  head
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    each css in htmlWebpackPlugin.files.css
      link(href=css, rel='stylesheet')

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:400,500,700|Roboto+Slab:400,700&amp;subset=latin-ext">    
    title Zbavte se závislosti | bezzavislosti.cz
  
  body
    include components/shared/svg-map.pug

    a(class="c-link-skip" href="#main") Přeskoč na hlavní obsah        

    include components/shared/navigation.pug      
    
    header(role="banner" class="c-header")
    
      include components/header.pug
    
    main(role="main" tabindex="-1")      

      div(id="main")

      section(role="region" tabindex="-1" id="what" class="u-no-focus")
    
        include components/what.pug
    
      section(role="region" tabindex="-1" id="how" class="u-no-focus")
    
        include components/how.pug
    
      section(role="region" tabindex="-1" id="contact" class="u-no-focus")
    
        include components/contact.pug
          
    footer(role="contentinfo" tabindex="-1" class="u-no-focus")
    
      include components/footer.pug 

    each js in htmlWebpackPlugin.files.js
      script(type='text/javascript', src=js)
    
    <script async src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAIQTeWItj-2H1Pwxe4pQQbpXWA8dZENBA&callback=initMap"></script>

      